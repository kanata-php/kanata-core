{
    "name": "kanata-php/kanata-core",
    "description": "Kanata App Starter Core",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kanata\\": "src/Kanata",
            "Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.1",
        "slim/slim": "4.*",
        "monolog/monolog": "^1.17",
        "league/flysystem": "^1.0",
        "guzzlehttp/guzzle": "^7.3",
        "symfony/validator": "^6.0",
        "league/pipeline": "^1.0",
        "kanata-php/socket-conveyor": "^1.1",
        "voku/php-hooks": "^0.2",
        "dflydev/fig-cookies": "^3.0",
        "doctrine/annotations": "^1.13",
        "doctrine/cache": "^1.10",
        "ray/aop": "^2.10",
        "psr/cache": "^2",
        "illuminate/database": "^8.83",
        "vlucas/phpdotenv": "^5.3",
        "ramsey/uuid": "^4.1",
        "phpseclib/phpseclib": "~3.0",
        "nesbot/carbon": "^2.48",
        "wikimedia/composer-merge-plugin": "^2.0",
        "league/plates": "^3.4",
        "ilexn/swoole-convert-psr7": "^0.6",
        "nyholm/psr7": "^1.4",
        "php-di/slim-bridge": "^3.2",
        "php-di/php-di": "^6.3",
        "aura/autoload": "^2.0",
        "symfony/console": "^5.4",
        "symfony/process": "^6.0",
        "mustache/mustache": "^2.14",
        "greg0/lazer-database": "^2.0",
        "php-amqplib/php-amqplib": "^3.1",
        "psy/psysh": "@stable",
        "danielstjules/stringy": "^3.1",
        "lotharthesavior/chocookies": "^0.0",
        "firebase/php-jwt": "^6.1"
    },
    "bin": [
        "start-kanata"
    ],
    "require-dev": {
        "symfony/var-dumper": "^5.2",
        "phpunit/phpunit": "^9.2",
        "codedungeon/phpunit-result-printer": "^0.30",
        "fakerphp/faker": "^1.19",
        "mockery/mockery": "^1.5"
    },
    "config": {
        "allow-plugins": {
            "wikimedia/composer-merge-plugin": true
        }
    },
    "suggest": {
        "ext-openswoole": "This package requires at least one of Open Swoole or Swoole",
        "ext-swoole": "This package requires at least one of Open Swoole or Swoole"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}
